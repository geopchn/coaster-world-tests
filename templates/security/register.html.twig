{% extends 'base.html.twig' %}

{% block title %}
    Inscription - {{ parent() }}
{% endblock %}

{% block body %}
    <h1>Inscription</h1>

    {{ form_start(form) }}

    {{ form_row(form.email) }}
    {{ form_row(form.username) }}
    {{ form_row(form.plainPassword) }}
    {{ form_row(form.terms) }}

    {{ form_row(form.submit) }}
    {{ form_end(form) }}
{% endblock %}
